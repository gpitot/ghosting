{
  "name": "ghosting",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && yarn build:css",
    "preview": "vite preview",
    "watch:css": "npx tailwindcss -i ./src/style.css -o ./dist/output.css --watch",
    "build:css": "npx tailwindcss -m -i ./src/style.css -o ./dist/output.css"
  },
  "devDependencies": {
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
